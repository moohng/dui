  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "dependencies": {
    "@better-scroll/core": "^2.0.0-beta.10",
    "@better-scroll/slide": "^2.0.0-beta.10",
    "@moohng/dan": "^1.5.2",
    "ali-oss": "^6.10.0",
    "axios": "^0.19.2",
    "md5": "^2.3.0",
    "vconsole": "^3.3.4",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6"
  }
