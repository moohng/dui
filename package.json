    "build:lib": "rollup -c build/rollup.config.js",
    "lint": "vue-cli-service lint"
  },
  "main": "dist/dui.min.js",
  "module": "lib/dui/index.js",
  "browser": "dist/dui.min.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "dui",
    "ui",
    "vue",
    "component",
    "dialog",
    "toast"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:moohng/dui.git"
  },
  "publishConfig": {
