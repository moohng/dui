{
  "name": "@moohng/dui",
  "version": "1.4.0",
  "private": false,
  "description": "基于 Vue 2 的轻量级组件库",
  "author": "Kevin",
  "scripts": {
    "serve": "vue-cli-service serve",
    "demo": "live-server --no-browser --watch=demo,dist",
